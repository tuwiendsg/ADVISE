  <xs:complexType name="productPurchase"><xs:annotation><xs:documentation><![CDATA[The ProductPurchase class holds the base data for each product purchase.]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="active" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag indicating whether the product purchase is currently active]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="billingEntityUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the billing entity associated with the product purchase]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="customerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the customer associated with the product purchase]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="description" type="xs:string"><xs:annotation><xs:documentation><![CDATA[A textual description of the product purchase]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="endDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The end date/time of the product period]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="lastBillPeriod" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The end date/time of the last billing period]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="nextBillPeriod" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The start date/time of the next billing period]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="productID" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The underlying productid of the product offer which linked with the current purchase]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="productOfferUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the associated product offer]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="purchaseDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The date/time the product purchase was made]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="purchaseUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the product purchase]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="purchasedConfig" nillable="true" type="tns:productComponent"><xs:annotation><xs:documentation><![CDATA[This will holed the configuration of the resource at the purchased time. Note this can not be used for FQL filtering]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="referenceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the product purchase (deprecated please use purchaseUUID )]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="resourceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the resource associated with the product purchase]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="startDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The start date/time of the product purchase]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="vdcUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the VDC associated with the product purchase]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="productComponent"><xs:annotation><xs:documentation><![CDATA[<p>The ProductComponent class represents the components of a product</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="lastBillingTime" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The Last billing time of this compomenet when linked with a purchase, this is a readonly value]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="billingConfiguredValues" nillable="true" type="tns:value"><xs:annotation><xs:documentation><![CDATA[The list of configurable values inherited from the Billing Method]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="billingMethodUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The billing method which is selected for this component]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="componentTypeUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the product component type linked with the ProductComponent ]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="productConfiguredValues" nillable="true" type="tns:value"><xs:annotation><xs:documentation><![CDATA[The list of configurable values inherited from the Product Component Type]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="value"><xs:annotation><xs:documentation><![CDATA[<p>A value, whether configured or measured.</p><p>Note that key names have are to a certain extent magic: if the Value corresponds to a resource, for a resource, the key may use the following format: &lt;RESOURCETYPE-ENUM.field&gt;, which will automatically link it to the field name within the resource. For instance, to define ram in the server, use the key name SERVER.ram, which will automatically link to the ram value within the Server object. Alternatively, any alphanumeric string may be used</p><p>Note that Values cannot be queried via FQL.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="description" type="xs:string"><xs:annotation><xs:documentation><![CDATA[A user-friendly description given to the value]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="hidden" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[The boolean used to hide]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="key" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the configured value]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="measureType" type="tns:measureType"><xs:annotation><xs:documentation><![CDATA[The type of unit used with the value]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="name" type="xs:string"><xs:annotation><xs:documentation><![CDATA[A user-friendly name given to the value]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="readOnly" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[The value is read only]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="required" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[The value is required or not]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="validator" type="tns:valueValidator"><xs:annotation><xs:documentation><![CDATA[A Validator object. Only values that match the validator can be set.]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="value" type="xs:string"><xs:annotation><xs:documentation><![CDATA[Value that going to be measured or configured]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="valueValidator"><xs:annotation><xs:documentation><![CDATA[<p>The ValueValidator class represents a Value Validator object.</p><p>A Value Validator object tests whether or not a prospective value specified is acceptable in a Configured Value.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="validateString" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The validate string.]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="validatorType" type="tns:validatorType"><xs:annotation><xs:documentation><![CDATA[The validator type.]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="unitTransaction"><xs:annotation><xs:documentation><![CDATA[<p>The UnitTransaction class holds the base data for a unit transaction.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:pseudoResource">
        <xs:sequence>
          <xs:element minOccurs="0" name="billingEntityUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[Billing Entity UUID of the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="chargedFromDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The start of the period to which the subject matter of the transaction relates]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="chargedToDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The end of the period to which the subject matter of the transaction relates]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="closingBalance" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The closing balance after the unit transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="description" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The textual description of the transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="descriptionDetail" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The textual description of the detail of the transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="linkedPurchase" type="tns:productPurchase"><xs:annotation><xs:documentation><![CDATA[The Product Purchase which is linked with the Transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="linkedResourceName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the resource linked with the unit tranaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="linkedResourceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the resource linked with the unit tranaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="linkedServerName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the server linked with the unit tranaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="linkedServerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The server UUID, if the link resource was part of a server at that time]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="openingBalance" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The open balance before the unit transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="transactionAmount" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The amount of the unit transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="transactionDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The date on which the unit transaction occurred]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="unitsType" type="tns:unitType"><xs:annotation><xs:documentation><![CDATA[The unit type associated with the unit transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="vdcUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the vdc which is linked with the transaction]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="pseudoResource"><xs:annotation><xs:documentation><![CDATA[<p>The PseudoResource class represents a pseudo-resource</p><p>A pseudo-resource is an identifier that behaves as if it were a resource for the purposes of the product system, but cannot be generated as a real resource. An example is billing for a disk-fetch.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="resourceType" type="tns:resourceType"><xs:annotation><xs:documentation><![CDATA[The type of the resource]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="unitTransactionSummary"><xs:annotation><xs:documentation><![CDATA[<p>The UnitTransactionSummary class holds the base data for each daily unit transaction summary.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="billingEntityUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[Billing Entity UUID of the customer]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="customerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID associated with the transaction summary]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="freeUnits" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The total free units within the summary period]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="transactionDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The date associated with the transaction summary]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="unitCredits" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The total credits within the summary period]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="unitDebits" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The total debits within the summary period]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="firewall"><xs:annotation><xs:documentation><![CDATA[<p>The Firewall class is a virtual resource holding data for a firewall instance.</p><p>Note that a firewall instance is an instance of a particular FirewallTemplate attached to a given IP address. The Firewall class itself does not hold firewall data.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:virtualResource">
        <xs:sequence>
          <xs:element minOccurs="0" name="ipAddress" type="xs:string"><xs:annotation><xs:documentation><![CDATA[the IP address to which the firewall is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="templateName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[the name of the firewall template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="templateUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[the UUID of the firewall template]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="virtualResource"><xs:annotation><xs:documentation><![CDATA[<p>The VirtualResource class is a resource is a base class on which all other virtual resources are based.</p><p>A virtual resource is a resource normally associated with a customer, cluster, VDC and product offer.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="clusterName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the cluster in which the resource is located]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="clusterUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the cluster in which the resource is located]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the customer that owns the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the customer that owns the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="deploymentInstanceName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the deployment instance the resource is created from]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="deploymentInstanceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The uuid of the deployment instance the resource is created from]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="productOfferName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the product offer associated with the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="productOfferUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the product offer associated with the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="vdcName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the VDC in which the resource is contained]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="vdcUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the VDC in which the resource is contained]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="resource"><xs:annotation><xs:documentation><![CDATA[<p>The Resource class is a base class which is the fundamental building block from which all resources in the system are derived.</p><p>The Resource class provides resource naming, state management, metadata, and keys. Note that most virtual objects are derived from VirtualResource.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:pseudoResource">
        <xs:sequence>
          <xs:element minOccurs="0" name="billingEntityName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The Name of the Billing Entity to which the resource belongs]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="billingEntityUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the Billing Entity to which the resource belongs]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="resourceCreateDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The creation date of the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="resourceKey" nillable="true" type="tns:resourceKey"><xs:annotation><xs:documentation><![CDATA[The keys attached to the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="resourceMetadata" type="tns:resourceMetadata"><xs:annotation><xs:documentation><![CDATA[The metadata attached to the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="resourceName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="resourceState" type="tns:resourceState"><xs:annotation><xs:documentation><![CDATA[The state of the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="resourceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the resource (read-only)]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="resourceKey"><xs:annotation><xs:documentation><![CDATA[<p>The ResourceKey class provides a key which can be attached to a given resource.</p><p>Each key is essentially a key/value pair. In fact the key is a 4-tuple, in that it includes a weight, which if non-zero affects the affinity calculation for workload placement, and a type to indicate whether the key is a system key or a user key. Note that users cannot manipulate system keys.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="resourceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the resource to which the key is attached]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="name" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the key]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="type" type="tns:resourceKeyType"><xs:annotation><xs:documentation><![CDATA[The type of the key (system or user)]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="value" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The value set on the key]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="weight" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The affinity weight of the key (zero means none)]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="resourceMetadata"><xs:annotation><xs:documentation><![CDATA[<p>The ResourceMetadata class implements metadata which can be attached to resources</p><p>Metadata falls into four classes: private, public, restricted and system. These can each be set on any object, though server and image metadata tend to be the most useful. For more information, see the metadata section of the main manual.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="privateMetadata" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The private metadata]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="publicMetadata" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The public metadata]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="resourceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the resource to which the metadata is attached]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="restrictedMetadata" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The restricted metadata]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="invoice"><xs:annotation><xs:documentation><![CDATA[<p>The Invoice class holds the base data for each invoice.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element name="invoiceTotalInc" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The invoice total including tax]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="invoiceDate" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The date of the invoice as printed on the invoice]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="invoiceTaxAmt" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The amount of tax associated with the invoice]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="invoiceTotalExc" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The invoice total excluding tax]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="paidDate" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The UNIX TIMESTAMP when invoice was paid]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="invoiceNo" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The invoice number]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="invoiceItems" nillable="true" type="tns:invoiceItem"><xs:annotation><xs:documentation><![CDATA[The invoice items]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name associated with the invoice]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="testMode" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[The test mode]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerAddress" type="tns:address"><xs:annotation><xs:documentation><![CDATA[The customer address field ]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="billingAddress" type="tns:address"><xs:annotation><xs:documentation><![CDATA[The billing address of the invoice]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerVatNo" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The VAT number or tax reference of the customer / recipient of the invoice]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="billingEntityVatNo" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The VAT number or tax reference of the billing entity / invoicing party]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="taxRate" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The tax rate of the invoice]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="pdfRef" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The PDF reference of the invoice]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="paymentMethodInstanceName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the associated FDL code Payment Method]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="status" type="tns:invoiceStatus"><xs:annotation><xs:documentation><![CDATA[The invoice status]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="dueDate" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The invoice due date (UNIX TIMESTAMP)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="cutOffDate" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The invoice cut off date (UNIX TIMESTAMP)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="paymentMethodInstanceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The payment method instance uuid]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="currencyId" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The currency id of the invoice]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="creditNote" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[The boolean to indicate credit note]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="transactionUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="pdf" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The pdf format of the invoice]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="invoiceItem"><xs:annotation><xs:documentation><![CDATA[<p>The InvoiceItem class holds the base data for each line item on a invoice.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="description" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The invoice line description]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="invoiceItemTaxAmt" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The amount of tax associated with the invoice line]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="invoiceItemTotalExc" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The value associated with the invoice line (excluding tax)]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="invoiceItemTotalInc" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The invoice line total (including tax)]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="invoiceUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the invoice]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="productPurchaseUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[Purcshase UUID is the invoice item is linked with a purchase]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="quantity" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The quantity associated with the invoice item]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="taxRate" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The tax rate]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="unitPrice" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The unit price associated with the invoice item]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="address"><xs:annotation><xs:documentation><![CDATA[The Address class holds the details of each address.]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="address1" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The first line of the address]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="address2" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The second line of the address]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="address3" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The third line of the address or town ]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="address4" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The fourth line of the address or county ]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="address5" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The fifth line of the address or country ]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="address6" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The sixth line of the address or postal code ]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="customerDetails"><xs:annotation><xs:documentation><![CDATA[<p>The Customer Details class holds the details for each customer.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:customer">
        <xs:sequence>
          <xs:element minOccurs="0" name="organisationName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The organisation name for the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="vatNumber" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The VAT number or tax reference for the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="customerCardCheck" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag to indicate whether the customer requires credit card checks]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="warningLevel" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The unit level below which which a low balance warning should be sent]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="warningSent" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag to indicate the low balance warning has already been sent]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="timeZone" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The timezone associated with the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="vatExempt" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[The VAT exemption status of the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="users" nillable="true" type="tns:userDetails"><xs:annotation><xs:documentation><![CDATA[The users/contacts associated with the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="email" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The email address associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="address" type="tns:address"><xs:annotation><xs:documentation><![CDATA[The address details of the customer]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="customer"><xs:annotation><xs:documentation><![CDATA[<p>The Customer class is a virtual resource holding the data for each customer.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="status" type="tns:status"><xs:annotation><xs:documentation><![CDATA[The status of the customer (determining whether it is active etc.)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="limitsMap"><xs:annotation><xs:documentation><![CDATA[A map containing the system limitations of the customer]]></xs:documentation></xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" name="key" type="tns:limits"/>
                      <xs:element minOccurs="0" name="value" type="xs:int"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="carryOverBalance" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The customer's current carry-over unit balance]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="nonCarryOverBalance" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The customer's current non-carry-over unit balance]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the customer (which will be the same as the resource UUID)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="validatedString" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The Validated string  of the customer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="creditCustomer" type="tns:paymentType"><xs:annotation><xs:documentation><![CDATA[A flag to indicate whether the customer has a credit account]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="productOfferUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the product offer associated with the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="exceedCreditLimit" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Boolean indicating if customer exceeds the credit limit]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="exceedCutoffLimit" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Boolean indicating if customer exceeds the cuoff limit]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="userDetails"><xs:annotation><xs:documentation><![CDATA[The UserDetails class holds the base data for each user / contact.]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:user">
        <xs:sequence>
          <xs:element minOccurs="0" name="address" type="tns:address"><xs:annotation><xs:documentation><![CDATA[The address details associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="dayTimeNumber" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The daytime telephone number associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="eveningNumber" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The evening telephone number associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="faxNumber" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The fax number associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="mobileNumber" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The mobile number associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="organisation" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The organisation name associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="user"><xs:annotation><xs:documentation><![CDATA[<p>The User class holds the base data for each user / contact.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="email" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The email address associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="firstName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The first name of the contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="lastLoginTime" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The last login time associated with the user / contact]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="lastName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The last name of the contact]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="cluster"><xs:annotation><xs:documentation><![CDATA[<p>The Cluster class is a virtual resource that represents a cluster.</p><p>Note that a cluster at Jade level and at Tigerlily level are different types</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="systemCapabilities" type="tns:systemCapabilitySet"><xs:annotation><xs:documentation><![CDATA[The capabilities of the cluster]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="hypervisorConfig" type="tns:hypervisorConfig"><xs:annotation><xs:documentation><![CDATA[The hypervisor specific configuration definitions for this cluster]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hypervisorSettings" nillable="true" type="tns:hypervisorSetting"><xs:annotation><xs:documentation><![CDATA[The hypervisor settings for this cluster]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="defaultSettings" nillable="true" type="tns:hypervisorSetting"><xs:annotation><xs:documentation><![CDATA[The combined default and cluster hypervisor settings]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="createDefaultVDC" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[To create a default VDC on creation of a customer]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="systemCapabilitySet"><xs:annotation><xs:documentation><![CDATA[<p>The SystemCapabilitySet class describes the capabilities of the system or a subset thereof</p><p>This class is most often used to describe the capabilities of a cluster or VDC.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="cloning" nillable="true" type="tns:cloning"><xs:annotation><xs:documentation><![CDATA[Cloning system capability]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="email" type="tns:email"><xs:annotation><xs:documentation><![CDATA[Email capability]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="emulatedDevices" nillable="true" type="tns:emulatedDevices"><xs:annotation><xs:documentation><![CDATA[Disabling emulated device capability]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="maxLimit"><xs:annotation><xs:documentation><![CDATA[System limits capability]]></xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="key" type="tns:maxLimit"/>
                  <xs:element minOccurs="0" name="value" type="xs:int"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="networking" nillable="true" type="tns:networking"><xs:annotation><xs:documentation><![CDATA[Networking system capability]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="payment" nillable="true" type="tns:payment"><xs:annotation><xs:documentation><![CDATA[Payment system capability]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="publish" nillable="true" type="tns:publish"><xs:annotation><xs:documentation><![CDATA[Image publication system capability]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="signup" nillable="true" type="tns:signup"><xs:annotation><xs:documentation><![CDATA[Signup system capability]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="snapshotting" nillable="true" type="tns:snapshotting"><xs:annotation><xs:documentation><![CDATA[Snapshotting system capability]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="vnc" nillable="true" type="tns:vnc"><xs:annotation><xs:documentation><![CDATA[VNC handler capability]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="hypervisorConfig"><xs:annotation><xs:documentation><![CDATA[A mapping of hypervisor configuration keys to valid string values used to specify a set of hypervisor specific setting keys and their valid values.]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="config"><xs:annotation><xs:documentation><![CDATA[The configuration settings for the hypervisor]]></xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="key" type="xs:string"/>
                  <xs:element minOccurs="0" name="value" type="tns:valueValidator"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="defaultSettings"><xs:annotation><xs:documentation><![CDATA[The default settings for the hypervisor]]></xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="setting" type="tns:hypervisorSetting"/>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="hypervisorSetting"><xs:annotation><xs:documentation><![CDATA[<p>HypervisorSetting represents a hypervisor configuration setting.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="key" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The key use for the setting]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="value" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The value used for the setting]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="locked" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[State if the setting is locked]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="productOffer"><xs:annotation><xs:documentation><![CDATA[<p>The ProductOffer class holds the base data for each product offer.</p><p>The Product Offer describes how, to whom and at what charge a Product may be sold.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="productUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[Holds the UUID of the linked product]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="componentConfig" nillable="true" type="tns:productComponent"><xs:annotation><xs:documentation><![CDATA[Holds the compoment configuration. Note that this cannot be queried using FQL]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="billingPeriod" type="tns:billingPeriod"><xs:annotation><xs:documentation><![CDATA[The billing period associated with the product offer (i.e how often a billing line is generated)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="unitType" type="tns:unitType"><xs:annotation><xs:documentation><![CDATA[The type of units associated with the product offer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="clusters" nillable="true" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The list of clusters with which Product Offer can be used]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="inUse" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Indicates if purchases already exists for this product offer]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="isEditable" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Indicates if this product offer is editable]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="productName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[Holds the name of the product]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="group"><xs:annotation><xs:documentation><![CDATA[<p>The Group class is a virtual resource representing a group.</p><p>A group is a collection of users; whilst a group belongs to a specific customer, note that a user exists independently of a specific customer.<p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:virtualResource">
        <xs:sequence>
          <xs:element minOccurs="0" name="type" type="tns:groupType"><xs:annotation><xs:documentation><![CDATA[The type of the group concerned]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="users" nillable="true" type="xs:string"><xs:annotation><xs:documentation><![CDATA[A collection of UUIDs of users that are members of this group]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="listResult"><xs:annotation><xs:documentation><![CDATA[<p>The ListResult class represents the result of a list call</p><p>The ListResult class contains both the results themselves, as well as a count of the total number of objects available, and indices of the first and last result returned.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="list" type="xs:anyType"><xs:annotation><xs:documentation><![CDATA[The result set]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="totalCount" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The total number of results available]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="listFrom" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The index of the first result returned within this result set]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="listTo" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The index of the last result returned within this result set]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="emailTemplate"><xs:annotation><xs:documentation><![CDATA[<p>The EmailTemplate class holds the base data for each email template.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="billingEmailVar"><xs:annotation><xs:documentation><![CDATA[A map containing the billing entity variables and strings for the email]]></xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="key" type="tns:billingEntityVAR"/>
                  <xs:element minOccurs="0" name="value" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="billingEntityUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The billing entity ID for the email template]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="body" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The body for the email]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="emailType" type="tns:emailType"><xs:annotation><xs:documentation><![CDATA[The email type for the email]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="enable" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Enables an email template]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="subject" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The subject line for the email]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="billingEntityDetails"><xs:annotation><xs:documentation><![CDATA[<p>The BillingEntityDetails class holds the base data for each Billing Entity.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:billingEntity">
        <xs:sequence>
          <xs:element minOccurs="0" name="address" type="tns:address"><xs:annotation><xs:documentation><![CDATA[The address details associated with the Billing Entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="vatNumber" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The VAT number / tax reference of the Billing Entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="controlPanelURL" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The public facing URL of the control panel for the Billing Entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="description" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The textual description of the Billing Entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="brand" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The numeric ID of the brand associated with the Billing Entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="currencyId" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The numeric currency ID associated with the Billing Entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="no3ds28DaySpendLimit" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The default monthly spend limit (in currency terms) associated with the Billing Entity for a non-3DS transaction]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="overall28DaySpendLimit" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The default overall monthly spend limit (in currency terms) associated with the Billing Entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="enableUserEditing" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Flag to enable editing of users]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="adminControlPanelURL" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The admin control panel URL of the Billing Entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="standardEmailSettings"><xs:annotation><xs:documentation><![CDATA[Map of email settings for the Billing Entity]]></xs:documentation></xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" name="key" type="tns:billingEntityVAR"/>
                      <xs:element minOccurs="0" name="value" type="xs:string"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element name="emailTemplates"><xs:annotation><xs:documentation><![CDATA[Map of email templates for the Billing Entity]]></xs:documentation></xs:annotation>
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" name="key" type="tns:emailType"/>
                      <xs:element minOccurs="0" name="value" type="tns:emailTemplate"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="invoiceSetting" type="tns:invoiceSetting"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="billingEntity"><xs:annotation><xs:documentation><![CDATA[<p>The BillingEntity class is a virtual resource holding data for each Billing Entity</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="billingType" type="tns:billingType"><xs:annotation><xs:documentation><![CDATA[The type of the billing entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="systemCapabilities" type="tns:systemCapabilitySet"><xs:annotation><xs:documentation><![CDATA[The system capabilities attached to this billing entity (note that it is not possible to filter by this field)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="permittedPOResourceTypes" nillable="true" type="tns:resourceType"><xs:annotation><xs:documentation><![CDATA[This holds lists of resource types which are allowed for a product offer to add/modify/delete]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="parentUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The immediate parent billing entity of this billing entity]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="descendants" nillable="true" type="xs:string"><xs:annotation><xs:documentation><![CDATA[A list of the uuids of all billing entities that descend from this billing entity]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="invoiceSetting"><xs:annotation><xs:documentation><![CDATA[A billing entity's settings for an invoice]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="dateFormat" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The date format string]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="timeZone" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The time zone]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="decimalPlace" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The number of decimal places to display]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="decimalSeparator" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The character to separate the decimal places]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="groupSeparator" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The character to separate group of thousand significant digits]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="tax" type="tns:tax"><xs:annotation><xs:documentation><![CDATA[The tax settings]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="invoiceHeaders"><xs:annotation><xs:documentation><![CDATA[A map of the invoice headers to the corresponding values to substitute]]></xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="key" type="tns:invoiceHeader"/>
                  <xs:element minOccurs="0" name="value" type="xs:string"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
      <xs:element minOccurs="0" name="pageSize" type="tns:pdfPageSize"><xs:annotation><xs:documentation><![CDATA[The paper size of the PDF version of the invoice]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="tax"><xs:annotation><xs:documentation><![CDATA[An object that represents the simplified tax settings for an invoice or credit note]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="taxRate" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The percentage of the total which is to be added as tax, as a value between 0.0 and 1.0.]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="taxName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the tax that is to be applied.]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="taxType" type="tns:taxType"><xs:annotation><xs:documentation><![CDATA[The type of tax that is to be applied.]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="jadeList"><xs:annotation><xs:documentation><![CDATA[<p>The JadeList class represents a list of strings</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="details" nillable="true" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The list of strings]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="keyList"><xs:annotation><xs:documentation><![CDATA[<p>The KeyList class represents a list of resource keys and their values</p><p>Note that as some key types can be multivalued, the value is represented as a JadeList type, i.e. a list of strings.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="content"><xs:annotation><xs:documentation><![CDATA[The map from key name to a list of values]]></xs:documentation></xs:annotation>
        <xs:complexType>
          <xs:sequence>
            <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
              <xs:complexType>
                <xs:sequence>
                  <xs:element minOccurs="0" name="key" type="xs:string"/>
                  <xs:element minOccurs="0" name="value" type="tns:jadeList"/>
                </xs:sequence>
              </xs:complexType>
            </xs:element>
          </xs:sequence>
        </xs:complexType>
      </xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="paymentMethod"><xs:annotation><xs:documentation><![CDATA[<p>The PaymentMethod class represents a Payment Method.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="paymentProviderUUID" type="xs:string"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="configuredValues" nillable="true" type="tns:value"><xs:annotation><xs:documentation><![CDATA[The list of configurable values which need to configured (These values are required by the specific payment provider)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="paymentProviderName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the associated payment provider]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="instanceValues" nillable="true" type="tns:value"><xs:annotation><xs:documentation><![CDATA[The list configurable values which need to configured when you are creating PaymentMethodInstace from this PaymentMethod]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="consolidateInvoices" type="xs:boolean"/>
          <xs:element minOccurs="0" name="configured" type="xs:boolean"/>
          <xs:element minOccurs="0" name="fdlPaymentRef" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The reference for the payment provider from the FDL code block.]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="paymentMethodInstance"><xs:annotation><xs:documentation><![CDATA[An instance of a payment method, mainly used by transactions.]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:customerResource">
        <xs:sequence>
          <xs:element minOccurs="0" name="paymentMethodUUID" type="xs:string"/>
          <xs:element name="isDefault" type="xs:boolean"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="configuredValues" nillable="true" type="tns:value"/>
          <xs:element minOccurs="0" name="paymentMethodName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the associated payment method]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="nonInteractivePay" type="xs:boolean"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="customerResource"><xs:annotation><xs:documentation><![CDATA[A resource that is owned by a customer resource.]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="customerName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the customer that owns the resource]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the customer that owns the resource]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="transaction"><xs:annotation><xs:documentation><![CDATA[An instance of a payment transaction against an invoice using a particular payment method.]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:customerResource">
        <xs:sequence>
          <xs:element minOccurs="0" name="paymentMethodInstanceUUID" type="xs:string"/>
          <xs:element minOccurs="0" name="state" type="tns:transactionState"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="invoiceUUIDList" nillable="true" type="xs:string"/>
          <xs:element minOccurs="0" name="providerReference" type="xs:string"/>
          <xs:element minOccurs="0" name="publicData" type="xs:string"/>
          <xs:element minOccurs="0" name="redirectURL" type="xs:string"/>
          <xs:element name="redirectData">
            <xs:complexType>
              <xs:sequence>
                <xs:element maxOccurs="unbounded" minOccurs="0" name="entry">
                  <xs:complexType>
                    <xs:sequence>
                      <xs:element minOccurs="0" name="key" type="xs:string"/>
                      <xs:element minOccurs="0" name="value" type="xs:string"/>
                    </xs:sequence>
                  </xs:complexType>
                </xs:element>
              </xs:sequence>
            </xs:complexType>
          </xs:element>
          <xs:element minOccurs="0" name="interactiveURL" type="xs:string"/>
          <xs:element name="allowInteractive" type="xs:boolean"/>
          <xs:element minOccurs="0" name="currencyCode" type="xs:string"/>
          <xs:element minOccurs="0" name="errorCode" type="xs:string"/>
          <xs:element minOccurs="0" name="errorReason" type="xs:string"/>
          <xs:element name="paymentAmount" type="xs:double"/>
          <xs:element minOccurs="0" name="paymentMethodInstanceName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the associated payment method instance]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="transactionFunction" type="tns:paymentFunction"/>
          <xs:element name="endTimestamp" type="xs:long"/>
          <xs:element name="startTimestamp" type="xs:long"/>
          <xs:element minOccurs="0" name="merchantDescription" type="xs:string"><xs:annotation><xs:documentation><![CDATA[A description of the transaction.]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="linkedTransaction" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of a linked transaction.]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="paymentCard"><xs:annotation><xs:documentation><![CDATA[<p>The PaymentCard object represents a payment card (e.g. credit card)</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="reference" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The payment provider's reference for the card concerned]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="authcode" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The payment provider's authentication for the card concerned]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="DSChecked" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag indicating whether secondary authentication (e.g. 3DS) has been performed]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="DSPassed" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag indicating whether secondary authentication (e.g. 3DS) has succeeded]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="avsPassed" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag indicating whether address verification has succeeded]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="avsDetail" type="xs:string"><xs:annotation><xs:documentation><![CDATA[Address verification details]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="scheme" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The card type (allowable values defined by the payment processor)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="expiryDate" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The expiry date in MM/YY format]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="registrationTime" type="xs:string"><xs:annotation><xs:documentation><![CDATA[[What is this and what is its format?]]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="declinedTransaction" type="xs:int"><xs:annotation><xs:documentation><![CDATA[Number of declined transactions]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="cardDefault" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Flag indicating whether the card is the default card]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="hash" type="xs:string"><xs:annotation><xs:documentation><![CDATA[Hash of card details]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="customerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[UUID of the customer associated with the card]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="paymentMethodUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[UUID of the payment method associated with the card]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="referralPromotion"><xs:annotation><xs:documentation><![CDATA[<p>The ReferralScheme class holds the base data for each referral scheme.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:promotion">
        <xs:sequence>
          <xs:element name="priority" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The priority of the referral scheme]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="invitorProductOfferUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The number of units the invitor receives on successful completion]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="inviteeMinimumPurchase" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The minimum unit purchase by the invitee to achieve successful completion]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="unitCost" type="xs:double"><xs:annotation><xs:documentation><![CDATA[The cost to the invitor's outstanding invitation balance]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="maxOutStanding" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The maximum number of outstanding instances of this referral promotion]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="promotion"><xs:annotation><xs:documentation><![CDATA[<p>The Promotion class holds the base data for each promotion.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:resource">
        <xs:sequence>
          <xs:element minOccurs="0" name="startDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The start date of the promotion code type]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="endDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The end date of the promotion code type]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="productOfferUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The numeric product offer ID associated with the promotion code]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="noCardCheck" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag to indicate whether a card check can be skipped with the promotion code]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="referralPromoCode"><xs:annotation><xs:documentation><![CDATA[<p>The ReferralPromoCode class holds the base data for each referral promo code.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:promoCode">
        <xs:sequence>
          <xs:element minOccurs="0" name="emailAddress" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The email address associated with the promo code]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="expiryDate" type="xs:dateTime"><xs:annotation><xs:documentation><![CDATA[The expiry date of the promo code]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="inviteeCustomerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The invitee's customer UUID with which it is associated]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="invitorCustomerUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The invitor's customer UUID with which it is associated]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="status" type="tns:referralStatus"><xs:annotation><xs:documentation><![CDATA[The referral status of the promo code]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="promoCode"><xs:annotation><xs:documentation><![CDATA[<p>The PromoCode class holds the base data for each promo code.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="promoCode" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The promotion code]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="promotionUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The promotion UUID]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="reuse" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag to indicate whether the promotion code can be reused]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="used" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag to indicate the promotion code has been used]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="dryRunResult"><xs:annotation><xs:documentation><![CDATA[<p>The class represents the result of dry run and list of resolved/unresolved references</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="resolvableReferences" type="tns:resolvableReference"><xs:annotation><xs:documentation><![CDATA[The list of references]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="success" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[success or failure]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="questions" type="tns:question"><xs:annotation><xs:documentation><![CDATA[The list of question]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="resolvableReference"><xs:annotation><xs:documentation><![CDATA[<p>The ResolvableResult class represents the result of resolved/unresolved references</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="referenceResource" type="tns:resource"><xs:annotation><xs:documentation><![CDATA[The reference resource]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="missingType" type="tns:resourceType"><xs:annotation><xs:documentation><![CDATA[The missing resourcetype]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="missingUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The missing resource uuid]]></xs:documentation></xs:annotation></xs:element>
      <xs:element maxOccurs="unbounded" minOccurs="0" name="alternateReferences" type="tns:resource"><xs:annotation><xs:documentation><![CDATA[List of alternate references to use]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="question"><xs:annotation><xs:documentation><![CDATA[<p>The Question object represents a question to be asked of the deployer when a Deployment Instance is deployed.</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="keyName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the key with which the question is associated]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="name" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The question to be asked]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="resource" type="tns:resource"><xs:annotation><xs:documentation><![CDATA[The resource to which the question relates]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="type" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The type of the reply to the question]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="deploymentTemplate"><xs:annotation><xs:documentation><![CDATA[<p>The DeploymentTemplate class is a template resource holding the details of resources which are deployed when the template is deployed.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:virtualResource">
        <xs:sequence>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="server" nillable="true" type="tns:server"><xs:annotation><xs:documentation><![CDATA[Holds the list of servers in a template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="network" nillable="true" type="tns:network"><xs:annotation><xs:documentation><![CDATA[Holds the list of networks in a template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="vdc" nillable="true" type="tns:vdc"><xs:annotation><xs:documentation><![CDATA[Holds the list of vdcs in a template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="firewallTemplate" nillable="true" type="tns:firewallTemplate"><xs:annotation><xs:documentation><![CDATA[Holds the list of firewall templates in a template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="firewall" nillable="true" type="tns:firewall"><xs:annotation><xs:documentation><![CDATA[Holds the list of firewalls in a template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="sshkey" nillable="true" type="tns:sshKey"><xs:annotation><xs:documentation><![CDATA[Holds the list of sshkeys in a template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="disk" nillable="true" type="tns:disk"><xs:annotation><xs:documentation><![CDATA[Holds the list of disks in a template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="deploymentTemplateUUID" type="xs:string"/>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="publishedTo" nillable="true" type="tns:publishTo"/>
          <xs:element minOccurs="0" name="userPermission" type="tns:templateProtectionPermission"><xs:annotation><xs:documentation><![CDATA[The user template permissions]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="ownerPermission" type="tns:templateProtectionPermission"><xs:annotation><xs:documentation><![CDATA[The owner template permissions]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="questions" nillable="true" type="tns:question"><xs:annotation><xs:documentation><![CDATA[Holds the List of questions in a template]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="instanceChargeProductOfferUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the product offer used to charge for the deployment instance on deploy]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="instanceChargeProductOfferName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the product offer used to charge for the deployment instance on deploy]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="server"><xs:annotation><xs:documentation><![CDATA[<p>The Server class is a virtual resource representing a server.</p><p>The server class is one of the most complex classes within the system.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:virtualResource">
        <xs:sequence>
          <xs:element name="cpu" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The number of virtual CPU cores assigned to the server]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="ram" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The amount of memory assigned to the server]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="initialUser" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The initial user name associated with the server (passed by metadata so a first boot process can initialise the operating system image)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="initialPassword" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The initial user name associated with the server (passed by metadata so a first boot process can initialise the operating system image)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="status" type="tns:serverStatus"><xs:annotation><xs:documentation><![CDATA[The status of the server (e.g. whether it is running or stopped)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="serverKey" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The server key of the server (for image/server cryptographic validation)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="imageUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the image from which the server was built]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="snapshotUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the snapshot from which the server was cloned]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="disks" nillable="true" type="tns:disk"><xs:annotation><xs:documentation><![CDATA[A list of the disks attached to the server]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="nics" nillable="true" type="tns:nic"><xs:annotation><xs:documentation><![CDATA[A list of the virtual NICs attached to the server]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="sshkeys" nillable="true" type="tns:sshKey"><xs:annotation><xs:documentation><![CDATA[A list of the sshkeys attached to the server]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="disableEmulatedDevices" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Instruct the hypervisor running the server to disable emulated devices or not. This will only work if the cluster has the DISABLE_EMULATED_DEVICE feature set. If the emulated devices are set to be disabled (default setting) the the server will be only presenetd with the para-virtualized devices. Any change to this value will need a server stop start]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="imageName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the image from which the server was built]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="imagePermission" type="tns:imagePermission"><xs:annotation><xs:documentation><![CDATA[This will contain the permission of the current image which is been used by the server, Note that you will not be able to use this field in FQL ]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="hypervisorSettings" nillable="true" type="tns:hypervisorSetting"><xs:annotation><xs:documentation><![CDATA[The hypervisor settings for this cluster]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="defaultSettings" nillable="true" type="tns:hypervisorSetting"><xs:annotation><xs:documentation><![CDATA[The combined default and cluster hypervisor settings]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="storageCapabilities" nillable="true" type="tns:storageCapability"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="disk"><xs:annotation><xs:documentation><![CDATA[<p>The Disk class is a virtual resource holding the data for each disk.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:virtualResource">
        <xs:sequence>
          <xs:element name="size" type="xs:long"><xs:annotation><xs:documentation><![CDATA[The capacity of the disk]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="status" type="tns:diskStatus"><xs:annotation><xs:documentation><![CDATA[The status of the disk]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="imageUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the OS Image from which the disk was derived]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="snapshotUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the snapshot from which the disk was derived]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="serverUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the server to which the disk is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="index" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The index of the disk (zero-based) specifying its position within the server to which it is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="imageName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the OS image from which the disk was derived]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="snapshotName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the snapshot from which the disk was derived]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="serverName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the server to which the disk is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="storageUnitUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the storage unit]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="iso" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Indicate if the disk in a iso or not]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="storageCapabilities" nillable="true" type="tns:storageCapability"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="nic"><xs:annotation><xs:documentation><![CDATA[<p>The Nic object is a virtual resource representing a virtual Network Interface Card (or NIC).</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:virtualResource">
        <xs:sequence>
          <xs:element name="index" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The zero-based index of the NIC within the server to which the NIC is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element maxOccurs="unbounded" minOccurs="0" name="ipAddresses" nillable="true" type="tns:ip"><xs:annotation><xs:documentation><![CDATA[The IP addresses associated with the NIC]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="macAddress" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The MAC address of the NIC]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="networkName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the network to which the NIC is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="networkType" type="tns:networkType"><xs:annotation><xs:documentation><![CDATA[The type of the network to which the Nic is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="networkUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the network to which the NIC is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="serverName" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The name of the server to which the NIC is attached]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="serverUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the server to which the NIC is attached]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

  <xs:complexType name="ip"><xs:annotation><xs:documentation><![CDATA[<p>The IP class holds the data about an IP address</p>]]></xs:documentation></xs:annotation>
    <xs:sequence>
      <xs:element name="auto" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag indicating whether the IP address is to be automatically assigned to the server (by DHCP or SLAAC)]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="ipAddress" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The IP address]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="nicUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the NIC to which the IP address is attached]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="subnetUUID" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The UUID of the subnet object in which the IP address is located]]></xs:documentation></xs:annotation></xs:element>
      <xs:element minOccurs="0" name="type" type="tns:ipType"><xs:annotation><xs:documentation><![CDATA[The type of the IP address (IPv4 or IPv6)]]></xs:documentation></xs:annotation></xs:element>
    </xs:sequence>
  </xs:complexType>

  <xs:complexType name="sshKey"><xs:annotation><xs:documentation><![CDATA[<p>The SSHKey class describes an ssh key.</p><p>SSHKeys are provided to servers via metadata. The normal use case is that they can initialise the SSH keys for the initial user on boot, thus permitted secure login without the danger inherent in an initial password.</p>]]></xs:documentation></xs:annotation>
    <xs:complexContent>
      <xs:extension base="tns:virtualResource">
        <xs:sequence>
          <xs:element name="globalKey" type="xs:boolean"/>
          <xs:element minOccurs="0" name="publicKey" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The public key in the same format as a line from authorized_keys]]></xs:documentation></xs:annotation></xs:element>
