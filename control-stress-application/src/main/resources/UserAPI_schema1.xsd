          <xs:element maxOccurs="unbounded" minOccurs="0" name="subnets" nillable="true" type="tns:subnet"><xs:annotation><xs:documentation><![CDATA[A list of the IP subnets associated with the network]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="ipv6RoutingEnabled" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[A flag to indicate whether IPv6 should be routedby the routing element of the network (if any)]]></xs:documentation></xs:annotation></xs:element>
        </xs:sequence>
      </xs:extension>
