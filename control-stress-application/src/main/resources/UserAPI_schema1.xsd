          <xs:element minOccurs="0" name="broadcastAddress" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The broadcast address (i.e. last, or all ones address) within the subnet]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="defaultGatewayAddress" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The default gateway]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="firstUsableAddress" type="xs:string"><xs:annotation><xs:documentation><![CDATA[The first usable address within the subnet]]></xs:documentation></xs:annotation></xs:element>
          <xs:element name="mask" type="xs:int"><xs:annotation><xs:documentation><![CDATA[The CIDR length of the subnet mask]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="subnetType" type="tns:ipType"><xs:annotation><xs:documentation><![CDATA[The type of the subnet (i.e. IPv4 or IPv6)]]></xs:documentation></xs:annotation></xs:element>
          <xs:element minOccurs="0" name="systemAllocated" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Indicates whether the subnet is allocated by the system or imported]]></xs:documentation></xs:annotation></xs:element>
