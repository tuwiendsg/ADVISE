      <xs:element name="canConsole" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Must be set if the image or disks/servers derived from it can have a console opened to them]]></xs:documentation></xs:annotation></xs:element>
      <xs:element name="canCreateServer" type="xs:boolean"><xs:annotation><xs:documentation><![CDATA[Must be set if the image or disks/servers derived from it can be used to create a server]]></xs:documentation></xs:annotation></xs:element>
